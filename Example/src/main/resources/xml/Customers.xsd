<xsd:schema elementFormDefault="qualified"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://sportshop.com/customers"
            xmlns:tns="http://sportshop.com/customer"
            xmlns="http://sportshop.com/customers">

    <xsd:import namespace="http://sportshop.com/customer" schemaLocation="Customer.xsd"/>

    <xsd:element name="customers">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="customer" type="tns:Customer" maxOccurs="unbounded" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>