<xs:schema elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://sportshop/orders"
           xmlns:tns="http://sportshop/order" xmlns="http://sportshop/orders">

    <xs:import namespace="http://sportshop/order" schemaLocation="Order.xsd" />

    <xs:element name="orders">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="order" type="tns:Order" maxOccurs="unbounded" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>