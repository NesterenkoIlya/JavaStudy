<xsd:schema elementFormDefault="qualified"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://sportshop/orders"
           xmlns:tns="http://sportshop/order"
           xmlns="http://sportshop/orders">

    <xsd:import namespace="http://sportshop/order" schemaLocation="Order.xsd" />

    <xsd:element name="orders">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="order" type="tns:Order" maxOccurs="unbounded" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>